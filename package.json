{
  "name": "unsplash-proxy",
  "version": "0.0.0",
  "description": "Proxy for the unsplash api",
  "main": "server.js",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "typescript": "^3.5.1",
    "unsplash-js": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.7",
    "@types/node-fetch": "^2.3.5",
    "@types/unsplash-js": "^5.0.0",
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "start": "node build/server.js",
    "start:watch": "nodemon",
    "build": "rm -rf dist && yarn compile",
    "compile": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "engines": {
    "node": "10.x"
  }
}